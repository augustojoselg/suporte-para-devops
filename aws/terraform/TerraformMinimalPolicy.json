{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ec2:*",
                "s3:*",
                "dynamodb:*",
                "iam:GetRole",
                "iam:ListRoles",
                "iam:PassRole",
                "iam:GetPolicy",
                "iam:ListPolicies",
                "cloudwatch:*",
                "logs:*",
                "autoscaling:*",
                "elasticloadbalancing:*",
                "kms:DescribeKey",
                "kms:ListAliases",
                "kms:ListKeys",
                "kms:Encrypt",
                "kms:Decrypt",
                "iam:CreateRole",
                "iam:CreatePolicy",
                "iam:AttachRolePolicy",
                "iam:CreateInstanceProfile",
                "iam:AddRoleToInstanceProfile",
                "iam:TagRole",
                "iam:GetPolicyVersion",
                "iam:ListPolicyVersions",
                "iam:ListAttachedRolePolicies",
                "iam:ListRolePolicies",
                "iam:DeleteRolePolicy",
                "iam:PutRolePolicy",
                "iam:DeleteRole",
                "iam:DeletePolicy",
                "iam:DeleteInstanceProfile",
                "iam:RemoveRoleFromInstanceProfile",
                "iam:DetachRolePolicy",
                "ecr:*",
                "elasticloadbalancing:*",
                "autoscaling:*",
                "cloudformation:*",
                "lambda:*",
                "apigateway:*",
                "route53:*",
                "acm:*",
                "waf:*",
                "shield:*",
                "guardduty:*",
                "config:*",
                "cloudtrail:*",
                "organizations:*",
                "sts:*",
                "secretsmanager:*",
                "ssm:*",
                "ecs:*",
                "eks:*",
                "rds:*",
                "elasticache:*",
                "redshift:*",
                "neptune:*",
                "docdb:*",
                "elasticsearch:*",
                "opensearch:*",
                "mq:*",
                "sqs:*",
                "sns:*",
                "events:*",
                "logs:*",
                "xray:*",
                "cloudwatch:*",
                "sagemaker:*",
                "comprehend:*",
                "transcribe:*",
                "translate:*",
                "rekognition:*",
                "textract:*",
                "polly:*",
                "lex:*",
                "connect:*",
                "pinpoint:*",
                "cognito-idp:*",
                "cognito-identity:*",
                "iam:*"
            ],
            "Resource": "*"
        }
    ]
}
